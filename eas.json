{
  "cli": {
    "version": ">= 16.9.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "cache": {
          "key": "noop"
        },
        "env": {
          "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
        },
        "node": "20.19.4",
        "image": "latest"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "cache": {
          "key": "noop"
        },
        "env": {
          "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
        },
        "node": "20.19.4",
        "image": "latest"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "cache": {
          "key": "noop"
        },
        "env": {
          "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
        },
        "node": "20.19.4",
        "image": "latest",
        "prebuildCommand": "npm install --legacy-peer-deps"
      }
    }
  },
  "submit": {
    "production": {}
  }
}